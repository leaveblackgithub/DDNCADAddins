<?xml version="1.0" encoding="utf-8"?>
<Project>
	<PropertyGroup Condition="'$(Configuration)|$(Platform)' == 'AcConsoleTest|AnyCPU'">
		<DefineConstants>DEBUG;TRACE;AcConsoleTest</DefineConstants>
	</PropertyGroup>
	<PropertyGroup Condition="'$(Configuration)|$(Platform)' == 'ApplicationTest|AnyCPU'">
		<DefineConstants>DEBUG;TRACE;ApplicationTest</DefineConstants>
	</PropertyGroup>
	<PropertyGroup>
		<RootNamespace>$(MSBuildProjectName)</RootNamespace>
		<AssemblyName>$(MSBuildProjectName)</AssemblyName>
		<Deterministic>false</Deterministic>
		<OutputPath>..\bin\</OutputPath>
	</PropertyGroup>
	<ItemGroup>
		<Compile Include="..\SharedAssemblyInfo.cs">
			<Link>SharedAssemblyInfo.cs</Link>
		</Compile>
		<Content Include="..\NLog.dll.nlog">
			<CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
		</Content>
		<None Include="..\NLog.xsd">
			<SubType>Designer</SubType>
		</None>
	</ItemGroup>
</Project>